	</PreferenceCategory><PreferenceCategory android:title="@string/category_copy_title"><Preference android:title="Copyright" android:summary="Andreas Pohl &lt;apohl79@gmail.com&gt;" android:key="copyright" /></PreferenceCategory>
	
